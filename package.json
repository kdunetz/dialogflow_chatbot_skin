{
  "name": "dialogflow-chatbot-skin",
  "description": "A simple Node.js based web app which shows how to use the Dialogflow API.",
  "version": "0.1.1",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test-integration": "casperjs test ./test/integration/test.*.js",
    "test-integration-runner": "NODE_ENV=test node casper-runner.js",
    "test": "npm run lint && npm run test-unit && npm run test-integration-runner",
    "test-unit": "istanbul cover ./node_modules/mocha/bin/_mocha test/unit",
    "lint": "eslint .",
    "autofix": "eslint --fix .",
    "codecov": "npm run test && (codecov || true)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kdunetz/dialogflow-chatbot-skin.git"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/bigquery": "^2.0.5",
    "@google-cloud/language": "^2.0.0",
    "@google-cloud/speech": "^2.1.1",
    "@google-cloud/storage": "^2.3.4",
    "@google-cloud/text-to-speech": "^0.4.0",
    "@google-cloud/translate": "^2.1.3",
    "body-parser": "^1.20.1",
    "dialogflow": "^0.7.0",
    "dotenv": "^2.0.0",
    "express": "^4.18.2",
    "googleapis": "^39.2.0",
    "striptags": "latest"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "casperjs": "^1.1.3",
    "codecov": "^1.0.1",
    "eslint": "^2.8.0",
    "istanbul": "^0.4.2",
    "mocha": "^10.2.0",
    "phantomjs-prebuilt": "^2.1.16",
    "supertest": "^1.2.0"
  },
  "engine": {
    "node": ">= 6.9.x",
    "npm": "> 3.10.x"
  }
}
